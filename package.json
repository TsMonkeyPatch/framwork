{
  "name": "@tsmonkeypatch/lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve sandbox",
    "build": "ng build ui",
    "postbuild": "node ./bin/postbuild.js && npm run package && npm run install:library",
    "package": "tar -cvf ./dist/ui.tar.gz -C dist/ui .",
    "postinstall": "npm run build && npm i --save ./dist/ui.tar.gz",
    "install:library": "npm i --save ./dist/ui.tar.gz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.6",
